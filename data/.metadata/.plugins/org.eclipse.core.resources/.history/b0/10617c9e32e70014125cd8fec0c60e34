package Education;

import java.util.Scanner;

import Grade.*;

public class useStudent {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		Student st = new Student("", "", 0.0, "", "");
		
		for (int i = 1; i <= 3; i++) {
			System.out.print("Enter ID : ");
			st.Id = scan.next();	
		}
		
	}
}
